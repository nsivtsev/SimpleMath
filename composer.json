{
    "type": "symfony-bundle",
    "name": "nsivtsev/simple-math",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "nsivtsev\\SimpleMath\\": "src/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
